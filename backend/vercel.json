{
    "installCommand": "npm install --lagacy-peer-deps",
    "verson": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/"
        }
    ]
}